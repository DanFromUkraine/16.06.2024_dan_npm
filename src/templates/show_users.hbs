<div class="users_container">
    {{#each this}}
        <div class="user">
            <div class="img"><img src="https://cdn-icons-png.flaticon.com/512/1144/1144760.png" alt="" style="width: 30px;" class="img-js"></div>
            <p>Email: {{this.Email}}</p>
            <p>FirstName: {{this.Firstname}}</p>
            <p>LastName: {{this.Lastname}}</p>
        </div>
    {{/each}}
</div>